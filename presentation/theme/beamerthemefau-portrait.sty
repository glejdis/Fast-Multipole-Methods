%%% Frametitle %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\setbeamertemplate{frametitle}
{%
  \begin{beamercolorbox}[wd=\dimexpr\paperwidth-\fau@outer@absenderraum-\fau@outer@linienstaerke,
                         ht=\dimexpr\fau@outer@absenderhoehe-\fau@outer@linienstaerke
                         ]{frametitle}
    \vbox to\dimexpr\fau@outer@absenderhoehe-\fau@outer@linienstaerke{%
      \vskip\beamer@leftmargin
      \hskip\beamer@leftmargin
      \begin{beamercolorbox}[wd=\dimexpr\paperwidth-2\fau@outer@absenderraum-\fau@outer@linienstaerke]{frametitle author}%
        \ifx\insertauthor\empty\else%
          {\Large\textbf{\insertauthor}}%
          \ifx\insertinstitute\empty
            \\[1.5ex]
          \else
            \\[-.5ex]
          \fi
        \fi%
        \ifx\insertinstitute\empty\else%
          {\Large\textmd{\insertinstitute}}\\[1.5ex]%
        \fi%
      \end{beamercolorbox}

      \hskip\beamer@leftmargin
      \begin{beamercolorbox}[wd=\dimexpr\paperwidth-2\fau@outer@absenderraum-\fau@outer@linienstaerke]{frametitle title}%
        {\usebeamerfont{title}\inserttitle}%
        \ifx\insertframesubtitle\empty\else%
          \par{\usebeamerfont{subtitle}\insertframesubtitle}%
        \fi%
      \end{beamercolorbox}%
      \vskip\beamer@leftmargin
    }%
  \end{beamercolorbox}
}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%% Footline %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\setbeamertemplate{footline}
{%
  \hskip\dimexpr\fau@outer@absenderraum+\fau@outer@linienstaerke-\fau@outer@outlinestaerke%
  \begin{beamercolorbox}[wd=\dimexpr\paperwidth-\fau@outer@absenderraum-\fau@outer@linienstaerke+\fau@outer@outlinestaerke,ht=\fau@outer@outlinestaerke,right]{outline}
  \end{beamercolorbox}

  \begin{beamercolorbox}[wd=\paperwidth]{footline}
    \begin{minipage}[c][\fau@outer@urlraum]{\paperwidth}
      \hskip\dimexpr\fau@outer@absenderraum+\fau@outer@linienstaerke
      {\normalsize\textbf{\fau@url}}
      \hfill
      {\normalsize\textbf{\fau@signature}}
      \hspace{\beamer@rightmargin}
    \end{minipage}
  \end{beamercolorbox}
}
